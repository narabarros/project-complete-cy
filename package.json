{
  "name": "mktzap-automation",
  "version": "1.0.0",
  "description": "Automatização do Mktzap - Smoke",
  "main": "index.js",
  "scripts": {
    "cypress": "cypress open",
    "report": "cypress run --config video=false --reporter mochawesome",
    "report:headed": "cypress run --headed --config video=false --reporter mochawesome",
    "spec": "cypress run --config video=false --reporter mochawesome --spec",
    "spec:headed": "cypress run --headed --config video=false --reporter mochawesome --spec",
    "test": "cypress run --headed"
  },
